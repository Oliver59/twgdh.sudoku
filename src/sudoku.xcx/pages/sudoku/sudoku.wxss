    .title {}

    .content {
        margin               : 0 auto;
        width                : 720rpx;
        height               : 720rpx;
        display              : grid;
        grid-template-rows   : repeat(3, 1fr);
        grid-template-columns: repeat(3, 1fr);
        border               : 2rpx solid #000;
    }

    .content .block_large {
        border               : 2rpx solid #000;
        display              : grid;
        grid-template-rows   : repeat(3, 1fr);
        grid-template-columns: repeat(3, 1fr);
    }

    .content .block_large .block_middle {
        border-right : 2rpx solid #bec4d1;
        border-bottom: 2rpx solid #bec4d1;
        display      : grid;
        grid-template-rows   : 100%;
        grid-template-columns: 100%;
    }

    .content .block_large .block_middle:nth-child(n+7) {
        border-bottom-width: 0;
    }

    .content .block_large .block_middle:nth-child(3n) {
        border-right-width: 0;
    }

    .content .block_large .block_middle .sudoku_number {
        color      : #2b2c30;
        font-size  : 48rpx;
        place-self: center center;
    }

    .content .block_large .block_middle .notes{
        display: grid;
        grid-template-rows   : repeat(3, 1fr);
        grid-template-columns: repeat(3, 1fr);
        place-items: center center;
        font-size: 18rpx;
        color: #808080;
    }

