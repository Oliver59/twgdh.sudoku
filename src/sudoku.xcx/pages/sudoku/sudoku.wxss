    .main {
        background-color: #fefefe;
        width: 720rpx;
        margin: 0 auto;
        /* font-family  :  'Times New Roman', serif; */
    }
    
    .sudoku {}
    
    .title {}
    
    .content {
        height: 720rpx;
        display: grid;
        grid-template-rows: repeat(9, 1fr);
        grid-template-columns: repeat(9, 1fr);
        /* border               : 2rpx solid #000; */
    }
    
    .content .block_large {
        border: 2rpx solid #000;
        display: grid;
        grid-template-rows: repeat(3, 1fr);
        grid-template-columns: repeat(3, 1fr);
    }
    
    .sudoku_block {
        border-right: 2rpx solid #bec4d1;
        border-bottom: 2rpx solid #bec4d1;
        display: grid;
        grid-template-rows: 100%;
        grid-template-columns: 100%;
    }
    
    .sudoku_block:nth-child(3n) {
        border-right: 2rpx solid #000;
    }
    
    .sudoku_block:nth-child(3n+1) {
        border-left: 2rpx solid #000;
    }
    
    .sudoku_block:nth-child(-n+9),
    .sudoku_block:nth-child(-n+36):nth-child(n+28),
    .sudoku_block:nth-child(-n+63):nth-child(n+55) {
        border-top: 2rpx solid #000;
    }
    
    .sudoku_block:nth-child(-n+27):nth-child(n+19),
    .sudoku_block:nth-child(-n+54):nth-child(n+46),
    .sudoku_block:nth-child(n+73) {
        border-bottom: 2rpx solid #000;
    }
    
    .sudoku_block:nth-child(-n+9) {
        border-top: 4rpx solid #000;
    }
    
    .sudoku_block:nth-child(n+73) {
        border-bottom: 4rpx solid #000;
    }
    
    .sudoku_block:nth-child(9n) {
        border-right: 4rpx solid #000;
    }
    
    .sudoku_block:nth-child(9n+1) {
        border-left: 4rpx solid #000;
    }
    
    .sudoku_block .sudoku_number {
        color: #2b2c30;
        font-size: 48rpx;
        place-self: center center;
    }
    
    .content .sudoku_block .notes {
        display: grid;
        grid-template-rows: repeat(3, 1fr);
        grid-template-columns: repeat(3, 1fr);
        place-items: center center;
        font-size: 18rpx;
        color: #808080;
    }
    
    .sudoku_block.sameBlock {
        background-color: #e1e5eb;
    }
    
    .sudoku_block.sameNumber {
        background-color: #cccedb;
    }
    
    .sudoku_block.selected {
        background-color: #c3dafa;
    }
    
    .sudoku_block.errorBlock {
        background-color: #f7cfd6;
    }
    
    .sudoku_block .sudoku_number.notSystemNumber {
        color: #3b59a9;
    }
    
    .sudoku_block .sudoku_number.error {
        color: #fb3d3f;
    }
    
    .sudoku_block.finishAnimation1 {
        background-color: #c8d5e4;
    }
    
    .sudoku_block.finishAnimation2 {
        background-color: #becee1;
    }
    
    .sudoku_block.finishAnimation3 {
        background-color: #b9cbdf;
    }
    
    .menu {
        width: 100%;
        height: 120rpx;
        display: grid;
        grid-template-columns: repeat(4, 1fr);
        place-items: center;
    }
    
    .number {
        color: #4f6ab1;
        font-size: 90rpx;
        display: grid;
        grid-template-columns: repeat(9, 1fr);
        place-content: center;
        place-items: center;
    }