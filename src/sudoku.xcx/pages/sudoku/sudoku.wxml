<view class="main">
    <view class="sudoku">
        <view class="title">
        </view>
        <view class="content">
            <block wx:for="{{sudoku.bindItemList}}" wx:for-item="sudukuItem">
                <view class="sudoku_block">
                    <view wx:if="{{sudukuItem.value!=0}}" class="sudoku_number">
                        {{sudukuItem.value}}
                    </view>
                    <view wx:else class="notes">
                        <view wx:for="{{9}}" wx:for-index="index" class="note_block">
                            {{index}}
                        </view>
                    </view>
                </view>
            </block>
        </view>
        <view class="menu">

        </view>
        <view class="number">

        </view>
    </view>
</view>